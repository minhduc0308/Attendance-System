SELECT * FROM ClassSession WHERE idClassSession = 1;
SELECT 
                ses.idClassSession, 
                ses.Date, 
                ses.Status, 
                lec.idLectures,
                class.idClass, 
                class.Name AS ClassName,
                class.StartDate, 
                class.EndDate, 
                r.rid, 
                r.rname
            FROM 
                ClassSession ses
           INNER JOIN
                Lectures lec ON lec.idLectures = ses.LectureId
            INNER JOIN 
                Class class ON class.idClass = ses.ClassId 
                INNER JOIN 
                Room r ON r.rid = ses.rid 
            WHERE 
               ses.LectureId = 1;
                
               Select ses.idClassSession, r.rid From ClassSession ses INNER JOIN Lectures lec ON lec.idLectures = ses.LectureId INNER JOIN Room r ON r.rid = ses.rid
